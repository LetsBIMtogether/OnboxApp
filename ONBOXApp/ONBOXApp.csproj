<Project Sdk="Microsoft.NET.Sdk">
	
	<PropertyGroup>
		<GenerateAssemblyInfo>false</GenerateAssemblyInfo>
	</PropertyGroup>
	
	<PropertyGroup>
		<NoWarn>$(NoWarn);CS1591;CS8019;CS0618;CS0168;CS0219</NoWarn>
		<WarningsAsMessages>$(WarningsAsMessages);*</WarningsAsMessages>
	</PropertyGroup>
	
	<PropertyGroup>
		<Configurations>Debug;Debug One;Release</Configurations>
		<Platforms>x64</Platforms>
		<OutputType>Library</OutputType>
		<RootNamespace>ONBOXAppl</RootNamespace>
		<AssemblyName>ONBOXAppl</AssemblyName>
		<RuntimeIdentifiers>win</RuntimeIdentifiers>

		<TargetFrameworks>net47;net48;net8.0-windows;</TargetFrameworks>		
		<RevitVersion Condition=" '$(RevitVersion)' == '' ">2024</RevitVersion>
		<DefineConstants>REVIT$(RevitVersion)</DefineConstants><!--;WINFORMS-->
		<PreprocessorDefinitions>$(DefineConstants)</PreprocessorDefinitions>
		
		<PlatformTarget>x64</PlatformTarget>
		<ResolveAssemblyWarnOrErrorOnTargetArchitectureMismatch>None</ResolveAssemblyWarnOrErrorOnTargetArchitectureMismatch>
		<DebugEngines>{351668CC-8477-4fbf-BFE3-5F1006E4DB1F}</DebugEngines>
		<EnableDefaultCompileItems>false</EnableDefaultCompileItems>
		<EnableDefaultItems>false</EnableDefaultItems>
		<AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
		<OutputPath>..\bin\$(Configuration)\$(RevitVersion)</OutputPath>
		<BuildInParallel>false</BuildInParallel>
	</PropertyGroup>
	
	<!-- Map Revit versions to .NET frameworks -->
	
	<PropertyGroup Condition="'$(TargetFramework)' == 'net47'">
		<RevitVersion>2019</RevitVersion>
		<OutputPath>..\bin\$(Configuration)\$(RevitVersion)</OutputPath>
		<!--<OutputPath>bin\$(Configuration)\$(RevitVersion)</OutputPath>-->
	</PropertyGroup>
	
	<PropertyGroup Condition="'$(TargetFramework)' == 'net8.0-windows'">
		<RevitVersion>2025</RevitVersion>
		<UseWPF>true</UseWPF>
		<OutputPath>..\bin\$(Configuration)\$(RevitVersion)</OutputPath>
	</PropertyGroup>
	
	<!-- Debug One configuration targets net48 for a single Revit version (e.g., 2024) -->
	<PropertyGroup Condition="'$(Configuration)' == 'Debug One'">
		<TargetFrameworks>net47</TargetFrameworks>
	</PropertyGroup>
	
	<!-- Debug-specific settings -->
	<PropertyGroup Condition="$(Configuration.Contains('Debug'))">
		<DefineConstants>$(DefineConstants);DEBUG</DefineConstants>
		<DebugType>full</DebugType>
		<DebugSymbols>true</DebugSymbols>
	</PropertyGroup>
	
	<!-- Release-specific settings -->
