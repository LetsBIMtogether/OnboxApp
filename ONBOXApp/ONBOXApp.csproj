	<!-- Add-in manifest processing -->
	<ItemGroup>
		<Content Include="Properties\launchSettings.json" />
	</ItemGroup>
	
	<!-- Prevent Revit SDK copy-local -->
	<Target Name="PreventRevitSDKCopyLocal" AfterTargets="ResolveReferences">
		<ItemGroup>
			<ReferenceCopyLocalPaths Remove="@(ReferenceCopyLocalPaths)" Condition="'%(ReferenceCopyLocalPaths.NuGetPackageId)' == 'Revit_All_Main_Versions_API_x64'" />
		</ItemGroup>
	</Target>
	
	<!-- Remove .deps.json for .NET 8.0 builds -->
	<Target Name="RemoveRevit2025DepsJsonFile" AfterTargets="AfterBuild" Condition="'$(RevitVersion)' == '2025'">
		<ItemGroup>
			<DepsJsonFile Include="$(OutputPath)\$(AssemblyName).deps.json" />
		</ItemGroup>
		<Delete Files="@(DepsJsonFile)" />
	</Target>
	
	<!-- Post-build event for copying to Revit Addins folder (Debug) -->
	<Target Name="CopyAddinsToAppData" AfterTargets="AfterBuild" Condition="$(Configuration.Contains('Debug'))">
		<Message Importance="high" Text="Copying addin file into Revit Addins folder for Revit $(RevitVersion)" />
		<ItemGroup>
			<AddinFiles Include="$(OutputPath)\*.addin" />
			<!--<OutputFiles Include="$(OutputPath)\*.dll;$(OutputPath)\*.pdb" />-->
		</ItemGroup>
		<MakeDir Directories="$(AppData)\Autodesk\Revit\Addins\$(RevitVersion)\ONBOX" Condition="!Exists('$(AppData)\Autodesk\Revit\Addins\$(RevitVersion)\ONBOX')" />
		<Copy SourceFiles="@(AddinFiles);@(OutputFiles)" DestinationFolder="$(AppData)\Autodesk\Revit\Addins\$(RevitVersion)\ONBOX" SkipUnchangedFiles="true" OverwriteReadOnlyFiles="true" Retries="3" RetryDelayMilliseconds="300" />
	</Target>
	
	<!-- Post-build event for copying to bundle (Release) -->
	<Target Name="CopyToBundle" AfterTargets="AfterBuild" Condition="'$(Configuration)' == 'Release'">
		<Message Importance="high" Text="Copying results to bundle for Revit $(RevitVersion)" />
		<ItemGroup>
			<OutputFiles Include="$(OutputPath)\*.dll;" />
			<AddinFile Include="$(MSBuildProjectDirectory)\ONBOXApp.addin" />
			<PackageFile Include="$(MSBuildProjectDirectory)\PackageContents.xml" />
		</ItemGroup>
		<MakeDir Directories="$(SolutionDir)OnboxApp.bundle\Contents\$(RevitVersion)\ONBOX" Condition="!Exists('$(SolutionDir)OnboxApp.bundle\Contents\$(RevitVersion)\ONBOX')" />
		<Copy SourceFiles="@(OutputFiles)" DestinationFolder="$(SolutionDir)OnboxApp.bundle\Contents\$(RevitVersion)\ONBOX" SkipUnchangedFiles="true" OverwriteReadOnlyFiles="true" Retries="3" RetryDelayMilliseconds="300" />
		<Copy SourceFiles="@(AddinFile)" DestinationFolder="$(SolutionDir)OnboxApp.bundle\Contents\$(RevitVersion)" SkipUnchangedFiles="true" OverwriteReadOnlyFiles="true" Retries="3" RetryDelayMilliseconds="300" />
		<Copy SourceFiles="@(PackageFile)" DestinationFolder="$(SolutionDir)OnboxApp.bundle" SkipUnchangedFiles="true" OverwriteReadOnlyFiles="true" Retries="3" RetryDelayMilliseconds="300" />
	</Target>

	<!-- Clean add-ins from AppData -->
	<Target Name="CleanAddinsInAppData" AfterTargets="AfterClean">
		<ItemGroup>
			<AddinFiles Include="$(AppData)\Autodesk\Revit\Addins\**\$(AssemblyName).*" />
		</ItemGroup>
		<Delete Files="@(AddinFiles)" />
	</Target>
	
	<!-- Post-dispatch builds for additional Revit versions -->
	<Target Name="Build2020" AfterTargets="DispatchToInnerBuilds">
		<Message Importance="high" Text="*** Running post-dispatch build for Revit 2020 ***" />
		<MSBuild Projects="$(MSBuildProjectFile)" Properties="Configuration=$(Configuration);TargetFramework=net47;RevitVersion=2020" />
	</Target>
	
	<Target Name="Build2021" AfterTargets="DispatchToInnerBuilds">
		<Message Importance="high" Text="*** Running post-dispatch build for Revit 2021 ***" />
		<MSBuild Projects="$(MSBuildProjectFile)" Properties="Configuration=$(Configuration);TargetFramework=net48;RevitVersion=2021" />
	</Target>
	
	<Target Name="Build2022" AfterTargets="DispatchToInnerBuilds">
