<Window x:Name="RenumberParkingClear" x:Class="ONBOXAppl.RenumberParkingCleanerUI"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:p="clr-namespace:ONBOXAppl.Properties"
        Icon="/ONBOXAppl;component/ResourcesWPF/onBox32.png"
        Title="{x:Static p:WindowLanguage.RenumberParkClear_Title}" Height="122.455" Width="271" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="10"/>
            <ColumnDefinition/>
            <ColumnDefinition Width="10"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="9"/>
            <RowDefinition/>
            <RowDefinition Height="13"/>
        </Grid.RowDefinitions>
        <GroupBox Grid.Column="1" Header="{x:Static p:WindowLanguage.RenumberParkClear_Options}" Grid.Row="1">
            <Grid>
                <Button x:Name="btnLevel" Content="{x:Static p:WindowLanguage.RenumberParkClear_Level}" HorizontalAlignment="Left" Margin="10,0,0,10" VerticalAlignment="Bottom" Width="93" Click="btnLevel_Click"/>
                <Button x:Name="btnAll" Content="{x:Static p:WindowLanguage.RenumberParkClear_All}" Margin="0,0,10,10" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="93" Click="btnAll_Click"/>
            </Grid>
        </GroupBox>

    </Grid>
</Window>
